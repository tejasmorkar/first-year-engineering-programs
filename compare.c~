#include<stdio.h>
void main(){
	int a, b, c;
	printf("Enter 3 different numbers\n");
	scanf("%d, %d, %d", &a, &b, &c);
	if(a > b && a > c)
		printf("%d is the greatest digit", a);
	else if(b > a && b > c)
		printf("%d is the greatest digit", b);
	else if(c > a && c > b)
		printf("%d is the greatest digit", c);
}
